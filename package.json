{
  "name": "gatsby-v2-starter",
  "description": "",
  "version": "0.0.1",
  "author": "",
  "scripts": {
    "build": "npm-run-all clean && gatsby build",
    "clean": "npm-run-all clean:*",
    "clean:cache": "rm -rf .cache",
    "clean:build": "rm -rf public",
    "develop": "gatsby develop",
    "lint": "eslint . --cache --max-warnings 0 && yarn run lint-scss",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "lint-scss": "sass-lint --config \".sass-lint.yml\" --max-warnings 0 -v",
    "start": "gatsby develop"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "git add",
        "eslint --cache --max-warnings 0"
      ],
      "*.scss": [
        "yarn lint-scss"
      ]
    },
    "ignore": [
      "node_modules/**/*.*",
      "src/styles/**/*.scss"
    ],
    "subTaskConcurrency": 1
  },
  "pre-commit": [
    "lint-staged"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.54",
    "@typeform/embed": "^0.5.8",
    "autoprefixer": "^8.4.1",
    "axios": "^0.18.0",
    "bowser": "^1.9.3",
    "classnames": "^2.2.5",
    "gatsby": "^2.0.70",
    "gatsby-image": "^2.0.25",
    "gatsby-plugin-favicon": "2.1.1",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.9",
    "i18n-iso-countries": "^3.7.3",
    "intersection-observer": "^0.5.0",
    "lodash.debounce": "^4.0.8",
    "lottie-web": "^5.1.15",
    "node-sass": "^4.9.0",
    "raven-js": "^3.26.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropdown": "^1.6.2",
    "react-helmet": "^5.2.0",
    "react-inlinesvg": "^0.8.1",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.4.0",
    "react-modal": "^3.4.5",
    "react-waypoint": "^8.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.2",
    "@babel/core": "^7.0.0-rc.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.54",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.54",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "7.0.0-beta.54",
    "@babel/plugin-proposal-logical-assignment-operators": "7.0.0-beta.54",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.2",
    "@babel/polyfill": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-flow": "^7.0.0-rc.2",
    "@babel/preset-react": "^7.0.0-rc.2",
    "@cobinhood/eslint-config-cobinhood": "^0.0.1",
    "babel-eslint": "^9.0.0",
    "babel-plugin-macros": "^2.4.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-styled-components": "^1.5.1",
    "chalk": "^2.4.1",
    "decompress": "^4.2.0",
    "decompress-unzip": "^4.0.1",
    "eslint": "^4.19.1",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-compat": "^2.3.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.70.0",
    "gatsby-plugin-styled-components": "^3.0.0-rc.0",
    "lint-staged": "^7.1.3",
    "node-fetch": "^2.2.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.0",
    "sass-lint": "^1.12.1",
    "styled-components": "^3.4.4"
  },
  "keywords": [
    "gatsby"
  ]
}
