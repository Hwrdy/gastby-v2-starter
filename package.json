{
  "name": "gatsby-v2-starter",
  "description": "",
  "version": "0.0.1",
  "author": "",
  "scripts": {
    "build": "npm-run-all clean && gatsby build",
    "clean": "npm-run-all clean:*",
    "clean:cache": "rm -rf .cache",
    "clean:build": "rm -rf public",
    "dev": "gatsby develop",
    "lint": "eslint . --cache --max-warnings 0 && yarn run lint-scss",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "lint-scss": "sass-lint --config \".sass-lint.yml\" --max-warnings 0 -v",
    "start": "gatsby develop"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "git add",
        "eslint --cache --max-warnings 0"
      ],
      "*.scss": [
        "yarn lint-scss"
      ]
    },
    "ignore": [
      "node_modules/**/*.*",
      "src/styles/**/*.scss"
    ],
    "subTaskConcurrency": 1
  },
  "pre-commit": [
    "lint-staged"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.0",
    "axios": "^0.18.0",
    "bowser": "^1.9.3",
    "gatsby": "^2.2.2",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-favicon": "3.1.5",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sharp": "^2.0.29",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-transformer-remark": "^2.3.7",
    "gatsby-transformer-sharp": "^2.1.17",
    "i18n-iso-countries": "^3.7.8",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.11.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-dropdown": "^1.6.4",
    "react-helmet": "^5.2.0",
    "react-inlinesvg": "^0.8.4",
    "react-intl": "^2.8.0",
    "react-loadable": "^5.4.0",
    "react-modal": "^3.8.1",
    "react-waypoint": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@cobinhood/eslint-config-cobinhood": "^0.2.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-macros": "^2.5.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "chalk": "^2.4.2",
    "decompress": "^4.2.0",
    "decompress-unzip": "^4.0.1",
    "eslint": "^5.15.3",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-compat": "^3.0.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "flow-bin": "^0.70.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "lint-staged": "^7.1.3",
    "node-fetch": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "sass-lint": "^1.12.1",
    "styled-components": "^4.1.3"
  },
  "keywords": [
    "gatsby"
  ]
}
